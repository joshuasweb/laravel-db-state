{
    "name": "joshuasweb/laravel-db-state",
    "description": "This is my package laravel-db-state",
    "keywords": [
        "joshuasweb",
        "laravel",
        "laravel-db-state"
    ],
    "homepage": "https://github.com/joshuasweb/laravel-db-state",
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua Caine",
            "email": "joshualcaine@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^8.73"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10",
        "orchestra/testbench": "^6.22",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Joshuasweb\\LaravelDbState\\": "src",
            "Joshuasweb\\LaravelDbState\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joshuasweb\\LaravelDbState\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Joshuasweb\\LaravelDbState\\LaravelDbStateServiceProvider"
            ],
            "aliases": {
                "LaravelDbState": "Joshuasweb\\LaravelDbState\\Facades\\LaravelDbState"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}